<%- include('header'); -%>
<div class="box" id="heading">
  <h1><%=titleName%></h1>
</div>
<div class="box">
  <% dataList.forEach((result)=> { %>
  <form action="/delete" method="post">
    <div class="item">
      <input type="checkbox" name="checkbox" value="<%=result._id%>" onchange="this.form.submit()"/> 
      <p><%=result.name%></p>
    </div>
    <input type="hidden" name="listname" value="<%=titleName%>"/> 
  </form>

  <% }); %>
  <form class="item" action="/" method="post">
    <input type="text" name="newItem" placeholder="New Item" required />
    <button type="submit" name="button" value="<%=titleName%>">+</button>
  </form>
</div>
<%- include('footer');-%>
